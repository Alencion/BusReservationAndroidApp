<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oreo.busreservation">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SearchBusActivity"></activity>
        <activity android:name=".BusListActivity"></activity>
        <activity android:name=".BusDetailActivity"></activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".PaymentActivity"></activity>
        <activity android:name=".BusRegisterActivity"></activity>
        <activity android:name=".MemberListActivity"></activity>
    </application>

</manifest>